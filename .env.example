# 环境变量配置示例文件
# 复制此文件为 .env 并填入您的实际配置信息

# OpenAI API 配置
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=your_openai_api_base_url_here
OPENAI_API_MODEL=gpt-4o

# 数据库配置
DB_HOST=your_database_host
DB_USER=your_database_username
DB_PORT=your_database_port
DB_PASSWORD=your_database_password
DB_NAME=your_database_name

# 备用数据库配置 (如果需要)
DB_HOST_YW=your_database_host_yw
DB_USER_YW=your_database_username_yw
DB_PORT_YW=your_database_port_yw
DB_PASSWORD_YW=your_database_password_yw
DB_NAME_YW=your_database_name_yw

# 其他配置
MAX_TOKENS=128000
RPM=10
TIMEOUT=10
TEMPERATURE=0.3
